[project]
    ide = xcode
[cxx]
    default_platform = iphonesimulator-x86_64
    ldflags = -framework Foundation -ObjC
    cflags = -g
[cache]
    mode = dir
[apple]
    default_debug_info_format_for_binaries = dwarf_and_dsym
    default_debug_info_format_for_libraries = dwarf_and_dsym
    default_debug_info_format_for_tests = dwarf_and_dsym
    iphonesimulator_target_sdk_version = 10.0